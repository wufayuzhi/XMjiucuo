{
  "name": "trae-project",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "check": "vue-tsc -b",
    "type-check": "vue-tsc -b --noEmit",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "node scripts/quality-check.cjs",
    "quality:fix": "npm run format && npm run lint:fix && npm run check",
    "start": "node mcp-server.js",
    "start-mcp": "node mcp-server.js",
    "start-web": "npm run dev",
    "start-all": "start-mcp.bat",
    "stop-all": "stop-mcp.bat",
    "health-check": "curl -f http://localhost:3001/api/status || exit 1",
    "install-mcp-deps": "npm install @modelcontextprotocol/sdk express ws chokidar"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "chokidar": "^3.6.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "lucide-vue-next": "^0.511.0",
    "tailwind-merge": "^3.3.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "vue-sonner": "^1.3.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^22.17.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/runtime-dom": "^3.4.15",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.33.0",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3",
    "unplugin-vue-dev-locator": "^1.0.0",
    "vite": "^5.0.12",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vue-tsc": "^1.8.27"
  }
}
